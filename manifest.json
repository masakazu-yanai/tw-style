{
  "name": "tw-style",
  "author": "Masakazu Yanai",
  "description": "chrome extension for twitter customize CSS and JS.",
  "version": "1.0.1",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["twitter/style.css"],
      "js":  ["twitter/script.js"]
    }
  ]
}