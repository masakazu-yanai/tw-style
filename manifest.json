{
  "name": "tw-style",
  "author": "Masakazu Yanai",
  "description": "chrome extension for twitter customize CSS and JS.",
  "version": "1.0.5",
  "manifest_version": 3,
  "manifest_version": 3,
  "web_accessible_resources": [{
    "resources": [
      "*"
    ],
    "matches": [
      "https://twitter.com/*"
    ]
  }],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["twitter/style.css"],
      "js":  ["twitter/script.js"]
    }
  ]
}